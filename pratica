 void main() {
 
  double nota1 = 2.0;
  double nota2 = 2.0;
  double nota3 = 0.0;
  
  double media = (nota1+nota2+nota3);
  
  if(media < 6){
    print("Reprovado");
  }else if(media == 7){
    print("Exame");
  }else if(media == 6){
    print("Exame");
  }else if(media > 7){
    print("Aprovado com louvor");
  }
   
}

