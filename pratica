void main() {
  
  var saldo_da_conta = 600;
  var saque = 800;
  
  var calculo = (saldo_da_conta - saque);
  var resultado1 = (saldo_da_conta > saque);
  var resultado2 = (saldo_da_conta < saque);
  var resultado3 = (saldo_da_conta == saque);
  
  
   if(resultado1){
    print ("Saque efetuado com sucesso!" + " " + "Agora você possui somente:" + " " + "$calculo");
  }else if (resultado2){
    print("Saldo indisponivel!" + " " + "Você possui apenas:" + " " + "$saldo_da_conta");
  }else if (resultado3){
    print ("Saque efetuado com sucesso!" + " " + "Agora você possui somente:" + " " + "$calculo");
  }
  
} 

