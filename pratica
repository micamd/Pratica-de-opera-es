void main() {
 
  double nota1 = 2.0;
  double nota2 = 3.0;
  
  double media = (nota1+nota2);
  
  if(media < 6){
    print("Reprovado");
  }else if(media == 6){
    print("Aprovado");
  }else if(media > 6){
    print("Aprovado com louvor");
  }
   
}

