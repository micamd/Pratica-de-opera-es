void main() {
  
  var peso = 70;
  
  if(peso < 60){
    print('Tamanho P');
  }else if(peso >= 61 && peso <= 74){
    print('Tamanho M');
  }else if(peso >= 75 && peso <= 85){
    print('Tamanho G');
  }else if (peso > 86){
    print('Tamanho GG');
  }
  
} 


