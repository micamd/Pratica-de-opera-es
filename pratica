 void main() {
  
  var idade = 22;
  
  if(idade >= 0 && idade <=12){
    print('Categoria criança');
  }else if(idade >= 13 && idade <= 21){
    print('Categoria Jovem');
  }else if(idade >= 22 && idade <= 59){
    print('Categoria Adulta');
  }else if (idade > 60){
    print('Categoria Terceira idade');
  }
  
} 

