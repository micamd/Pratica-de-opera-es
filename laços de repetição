void main() {
  
  double multaA = 21;
  double multaB = 14;
  double multaC = 22;
  
  print("Pontos na carteira:" " " + "$multaA");
  
  var valorA = ("O valor da sua multa é de: 500,00");
  var valorB = ("O valor da sua multa pode variar entre: 501,00 á 1,000");
  var valorC = ("O valor da sua multa é de: 1001,00");

  var multa1 = (multaA);
  var multa2 = (multaB);
  var multa3 = (multaC);
  
 if(multa1 <= 7){
    print("$valorA");
  }else if(multa1 <= 20){
    print("$valorB");
  }else if(multa1 >= 21){
    print("$valorC");
  }
  
   print("Pontos na carteira:" " " + "$multaB");
  
   if(multa2 <= 7){
    print("$valorA");
  }else if(multa2 <= 20){
    print("$valorB");
  }else if(multa2 > 21){
    print("$valorC");
  }
  
  print("Pontos na carteira:" " " + "$multaC");
  
   if(multa3 <= 7){
    print("$valorA");
  }else if(multa3 <= 20){
    print("$valorB");
  }else if(multa3 >= 21){
    print("$valorC");
  }
  
  var valor_multas1 = (500.00);
  var valor_multas2 = (700.00);
  var valor_multas3 = (1001.00);
  
  var calculo = (valor_multas1+valor_multas2+valor_multas3);
  print ("O detran recebe hoje:" + " " + "$calculo" + " " + "reais de multas");
}


