void main() {
  
  var numeros = [2,3,4,5,7,9,34,56,24,12];
  
   
  print ("LISTA DOS NÚMEROS");
  print (numeros); 
  

  print ("");
  
  int numeros = 0;
  
  for(int i = 100; i<= 100; i = numeros; i++){
  
  print ("O MAIOR NÚMERO É O:" + " " + i.toString());
  
    }
}
      
