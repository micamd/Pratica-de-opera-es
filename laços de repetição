7)

void main() {

  double media = 0;
  double maior = 2.15;
  double menor = 1.50;
  
  var numeros = [1.72, 1.80, 1.45, 1.94, 1.32, 1.67, 1.65, 1.43, 2.10, 2.05, 1.87, 1.82, 1.83, 1.75, 1.69];
  
  
  for (double num in numeros){
    media += num;
    maior == num;
    menor == num;
  }
  
  media = media/15;
  maior = maior=2.10;
  menor = menor=1.32;
  
  print ("ALTURA DOS JOGADORES");
  print (numeros);
  print ("");
  print ("MÉDIA DA ALTURA DOS JOGADORES É:" + " " + '${media.toStringAsFixed(2)}');
  print ("A MAIOR ALTURA É DE:" + " " + '${maior.toStringAsFixed(2)}');
  print ("A MENOR ALTURA É DE:" + " " + '${menor.toStringAsFixed(2)}');
}
 

