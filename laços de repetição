8)

void main() {

  int soma = 2;
  String result ='1';
  
  print ("NÚMERO SOMADO:" + " " + "$soma");
  
  for(int i = 1; i <= 3; i++){
    result += ' ${i *1}';
    soma = soma + i *2;
    print('NÚMERO * 2 = $soma');
  }
}

