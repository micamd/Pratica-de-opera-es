void main() {

 Map<int, String> candidato = new Map();

  candidato[1] = 'Chaves';
  candidato[2] = 'Nhonho';
  candidato[3] = 'Quico';
  candidato[4] = 'Seu madruga';
  candidato[5] = 'Girafales';
  candidato[6] = 'Jaiminho';
  candidato[7] = 'Seu barriga';
  
  print ("CANDIDATOS");
  print(candidato);
  
 Map<int, String> peso = new Map();

  peso[1] = '90';
  peso[2] = '95';
  peso[3] = '70';
  peso[4] = '45';
  peso[5] = '80';
  peso[6] = '35';
  peso[7] = '100';
  
  print ("PESO");
  print(peso);
  
  var pesado1 = (candidato[1]);
  var pesado2 = (candidato[2]);
  var pesado3 = (candidato[7]);
  
  print(" ");
  print("CATEGORIA PESO PESADO, CANDIDATOS ACEITOS:" + " " + "$pesado1" + "," + "$pesado2" + "," + "$pesado3");
  
}
  
