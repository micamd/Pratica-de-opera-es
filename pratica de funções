void main() {

   var num1 = 3;
   var num2 = 9;
   var num3 = 8;
   var num4 = 14;
   var num5 = 26;
   
   var mais = (num1+num2+num3+num4+num5);
   var divisao = (mais / 5);
   var soma = (mais+divisao);
   
   print ("O valor da média aritmédica é:" + " " + "$soma" );
 }

