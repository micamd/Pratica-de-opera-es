void main() {

   var vetor = [9, 5, 7, 2];
   var aux = 0;


            for (int i = 0; i < vetor.length; i++)
            {
                for (int j = 0; j < vetor.length; j++)
                {
                    if (vetor[i] < vetor[j])
                    {
   
                        aux = vetor[i];
                        vetor[i] = vetor[j];
                        vetor[j] = aux;
                    }
                }
            }
  
  var vetor1 = [9, 5, 7, 2];
   var aux1 = 0;


       for (int i = 0; i < vetor1.length; i++)
            {
                for (int j = 0; j < vetor1.length; j++)
                {
                    if (vetor1[i] > vetor1[j])
                    {
   
                        aux1 = vetor1[i];
                        vetor1[i] = vetor1[j];
                        vetor1[j] = aux1;
                    }
                }
            }

  print ("Ordem crescente: "+ " "+ "$vetor");
  print ("Ordem decrescente: "+ " "+ "$vetor1");
}
