   var num2 = 4;
   
   var calculo = (num1 + num2);
   var calculo2 = (num1 * num2*num2);
   var calculo3 = (num1 * num1);
   var calculo4 = (calculo * 6);
   var calculo5 = (calculo * 3);
   var calculo6 = (num1 |0|);
   
   print (calculo);
   print (calculo2);
   print (calculo3);
   print (calculo4);
   print (calculo5);
 }

