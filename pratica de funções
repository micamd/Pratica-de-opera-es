void main() {

  var idade = 20;
  
  if(idade >= 5 && idade <=7){
    print('Você tem:'+ " " + '$idade' + " " + "Anos" + " " + 'está na Categoria Infantil A');
  }else if(idade >= 8 && idade <= 10){
    print('Você tem:'+ " " + '$idade' + " " + "Anos" + " " + 'está na Categoria Infantil B');
  }else if(idade >= 11 && idade <= 13){
    print('Você tem:'+ " " + '$idade' + " " + "Anos" + " " + 'está na Categoria Juvenil A');
  }else if (idade >= 14 && idade <= 17){
    print('Você tem:'+ " " + '$idade' + " " + "Anos" + " " + 'está na Categoria Juvenil B');
  }else if (idade >= 18){
    print('Você tem:'+ " " + '$idade' + " " + "Anos" + " " + 'está na Categoria Adulto');
 }
 }

